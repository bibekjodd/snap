{
  "name": "@jodd/snap",
  "version": "1.0.8",
  "description": "State manager & query tool for react apps",
  "keywords": [
    "state management",
    "react-state"
  ],
  "homepage": "https://github.com/bibekjodd/snap",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bibekjodd/snap.git"
  },
  "license": "MIT",
  "author": "bibekjodd",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write .",
    "lint": "eslint src && tsc",
    "prepublishOnly": "tsup"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2"
  }
}
